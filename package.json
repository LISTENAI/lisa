{
  "name": "@listenai/lisa",
  "description": "Lisa 是 Lisa Framework 核心命令行工具，提供模版生成能力以及任务执行命令",
  "version": "2.0.0-beta.1",
  "author": "geekBoom.Zhao <zbzhao@listenai.com>",
  "bin": {
    "lisa": "./bin/run"
  },
  "bugs": "https://github.com/LISTENAI/lisa/issues",
  "dependencies": {
    "@listenai/lisa_core": "2.0.0-beta.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-plugins": "^1.10.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "public"
  ],
  "homepage": "https://github.com/LISTENAI/lisa.git",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lisa",
    "plugins": [
      "@oclif/plugin-plugins"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/load"
      ]
    },
    "userPlugins": false
  },
  "lisa": {
    "repository": [
      {
        "type": "github",
        "url": "https://github.com/LISTENAI/lisa.git"
      },
      {
        "type": "gitee",
        "url": "https://gitee.com/listenai/lisa.git"
      }
    ],
    "bugs": [
      {
        "type": "github",
        "url": "https://github.com/LISTENAI/lisa/issues"
      },
      {
        "type": "gitee",
        "url": "https://gitee.com/listenai/lisa/issues"
      }
    ]
  },
  "repository": "https://github.com/LISTENAI/lisa.git",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
