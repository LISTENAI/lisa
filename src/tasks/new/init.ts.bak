import PreNew from '../../lib/pre-new'
import {job, application} from '@listenai/lisa_core'
module.exports = () => {
  job('new:init_csk', {
    title: '配置初始化',
    task: async (ctx: { base: any }) => {
      const preNew = new PreNew()
      const base: any = await preNew.getBasicConf(application.newCtx.chip, application.newCtx.firmware, application.newCtx.hardware)
      ctx.base = base
    },
  })
}
